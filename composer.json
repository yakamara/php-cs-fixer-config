{
    "name": "yakamara/php-cs-fixer-config",
    "description": "php-cs-fixer config for Yakamara projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregor Harlan",
            "email": "gregor.harlan@yakamara.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "friendsofphp/php-cs-fixer": "^3.16",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Yakamara\\PhpCsFixerConfig\\": "src"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
